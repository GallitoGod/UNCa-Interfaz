{
  "model_type": "detection",

  "input": {
    "width": 640,
    "height": 640,
    "channels": 3,
    "normalize": true,
    "mean": [0.0, 0.0, 0.0],
    "std": [1.0, 1.0, 1.0],
    "letterbox": true,
    "auto_pad_color": [114, 114, 114],
    "preserve_aspect_ratio": true
  },

  "output": {
    "confidence_threshold": 0.5,
    "nms_threshold": 0.45,
    "apply_nms": true
  }
}
  