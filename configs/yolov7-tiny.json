
{
  "model_type": "detection",
  "input": {
    "width": 640,
    "height": 640,
    "channels": 3,
    "normalize": true,
    "mean": [0.0, 0.0, 0.0],
    "std": [1.0, 1.0, 1.0],
    "scale": true,
    "letterbox": true,
    "auto_pad_color": [114, 114, 114],
    "preserve_aspect_ratio": true,
    "color_order": "RGB",
    "input_tensor": {
      "layout": "NCHW",
      "dtype": "float32",
      "quantized": false
    }
  },
  "output": {
    "confidence_threshold": 0.25,
    "nms_threshold": 0.45,
    "apply_nms": false,
    "tensor_structure": {
      "box_format": "xyxy",
      "coordinates": {
        "x1": 0,
        "y1": 1,
        "x2": 2,
        "y2": 3
      },
      "confidence_index": 5,
      "class_index": 4,
      "num_classes": null
    },
    "output_tensor": {
      "output_format": "raw"
    }
  }
}