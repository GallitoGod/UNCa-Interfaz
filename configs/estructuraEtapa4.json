{
  "model_type": "detection",
  "input": {
    "width": 640,
    "height": 640,
    "channels": 3,
    "normalize": true,
    "mean": [0.0, 0.0, 0.0],
    "std": [1.0, 1.0, 1.0],
    "letterbox": true,
    "auto_pad_color": [114, 114, 114],
    "preserve_aspect_ratio": true
  },
  "output": {
    "confidence_threshold": 0.5,
    "nms_threshold": 0.45,
    "apply_nms": true,
    "tensor_structure": {
      "box_format": "xyxy",
      "box_indices": [0, 1, 2, 3],
      "confidence_index": 4,
      "class_index": 5,
      "class_count": 80
    }
  },
  "OBSERVACION": "esta seria la estructura que tendrian que tener los JSONs para poder funcionar"
}
